class Solution {
public:
    ListNode* reverseList(ListNode* head) {
        ListNode* current = head;
        ListNode* previous = NULL;
        while(head!=NULL){
            head = current ->next;
            current->next = previous;
            previous = current;
            current = head;
        }
        // head=NULL hoga , current = NULL hoga is point pe 
        return previous;
    }
};
